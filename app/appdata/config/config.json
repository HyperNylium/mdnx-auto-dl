{
    "monitor-series-id": [],
    "app": {
        "TEMP_DIR": "appdata/temp",
        "BIN_DIR": "appdata/bin",
        "LOG_FILE": "appdata/logs/app.log",
        "MDNX_API_FORCE_REAUTH": false,
        "MDNX_API_SKIP_TEST": false,
        "MDNX_SERVICE_USERNAME": "",
        "MDNX_SERVICE_PASSWORD": ""
    },
    "mdnx": {
        "bin-path": {
            "ffmpeg": "../ffmpeg/bin/ffmpeg.exe",
            "ffprobe": "../ffmpeg/bin/ffprobe.exe",
            "mkvmerge": "../mkvtoolnix/mkvmerge.exe",
            "mp4decrypt": "../Bento4-SDK/bin/mp4decrypt.exe"
        },
        "cli-defaults": {
            "q": 0,
            "kstream": 1,
            "server": 1,
            "partsize": 3,
            "fileName": "${seriesTitle} - S${season}E${episode}",
            "dubLang": [
                "eng",
                "jpn"
            ],
            "dlsubs": [
                "en"
            ],
            "defaultAudio": "jpn",
            "timeout": 30000,
            "waittime": 3000,
            "mp4": false,
            "nocleanup": false,
            "dlVideoOnce": false,
            "keepAllVideos": false
        },
        "dir-path": {
            "content": "./videos/",
            "fonts": "./fonts/"
        },
        "gui": {
            "port": 3000
        }
    },
    "dependency_urls": {
        "Bento4-SDK": "https://www.hypernylium.com/projects/mdnx-auto-dl/Bento4-SDK.zip",
        "ffmpeg": "https://www.hypernylium.com/projects/mdnx-auto-dl/ffmpeg.zip",
        "mdnx": "https://www.hypernylium.com/projects/mdnx-auto-dl/mdnx.zip",
        "mkvtoolnix": "https://www.hypernylium.com/projects/mdnx-auto-dl/mkvtoolnix.zip"
    }
}